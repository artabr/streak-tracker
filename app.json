{
  "expo": {
    "name": "Tend - habit tracker",
    "slug": "streak-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.artabr.streakTracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.artabr.streakTracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96ef969c-cb59-451a-826e-6cf9bd2643b9"
      }
    },
    "owner": "artabr"
  }
}
